    "class Rectangle:\n",
    "    def __init__(self,l,w):\n",
    "        self.l = l\n",
    "        self.w = w\n",
    "    def area(self):\n",
    "        return (self.l*self.w)\n",
    "    def perimeter(self):\n",
    "        return (2*self.l+ 2*self.w)\n",
    "        \n",
    "r1= Rectangle(3,4)\n",
    "print ('area result',r1.area())\n",
    "print ('preimi'r1.perimeter())"
