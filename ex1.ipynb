    "class Point3D:\n",
    "    def __init__(self, x, y, z):\n",
    "        self.x = x\n",
    "        self.y = y\n",
    "        self.z = z\n",
    "        \n",
    "    def ToString(self):\n",
    "        return \"({},{},{})\".format(self.x, self.y, self.z)\n",
    "        \n",
    "mypoints = Point3D(1,2,3)\n",
    "\n",
    "print (mypoints.ToString())"
